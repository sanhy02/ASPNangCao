@model PersonViewModel
<h2>THONG TIN DANG KY</h2>
<hr />
<form asp-action="XuLy" method="post" enctype="multipart/form-data">
	<div class="form-group">
		<label>Ho ten</label>
		<input asp-for="FullName" class="form-control" />
	</div>
	<div class="form-group">
		<label>Ngay sinh</label>
		<input asp-for="BirthDay" type="date" class="form-control" />
	</div>
	<div class="form-group">
		<label>Hinh dai dien</label>
		<input name="FHinh" type="file" class="form-control" />
	</div>
	<button type="submit" class="btn btn-primary">Dang ky</button>
</form>